{
  "name": "@eid-easy/eideasy-widget",
  "version": "0.7.0",
  "main": "dist/full/eideasy-widget.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist/full --target lib --inline-vue --name eideasy-widget src/main.js",
